{
  "name": "Elixir",
  "rules": {
    "program": {
      "type": "SYMBOL",
      "name": "expression"
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "module_definition"
        }
      ]
    },
    "dot": {
      "type": "STRING",
      "value": "."
    },
    "module_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "defmodule"
        },
        {
          "type": "SYMBOL",
          "name": "module_name"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        }
      ]
    },
    "module_name": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "\\.?[A-Z0-9][a-zA-Z0-9]*"
      }
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "do"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "expression"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "end"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

